{
    "version": "0.2.0",
    "configurations": [{
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "D:/ASP/asp.exe",
        "name": "LaunchAppTpl",
        "stopOnEntry": false,
        "outFiles": ["${workspaceRoot}/dist/**/*.js"],
        "args": ["--shellapp=AppTpl", "--newapp=${workspaceRoot}/dist"],
        "env": {
            "NODE_PATH": "D:\\ASP\\resources\\app\\node_modules",
            "dev": "AppTpl"
        },
        "protocol":"inspector",
        "trace":true,
        "console":"externalTerminal",
        "internalConsoleOptions": "openOnSessionStart"
    }]
}